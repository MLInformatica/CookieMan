// Require the necessary APIs
const logToConsole = require('logToConsole');
const injectScript = require('injectScript');
const gtagSet = require('gtagSet');

const fromBase64 = require('fromBase64');
const JSON = require('JSON');

const setDefaultConsentState = require('setDefaultConsentState');
let hasDefaultConsentState = false;
let consentDefaultStates = [];
const updateConsentState = require('updateConsentState');

const queryPermission = require('queryPermission');
const getTimestampMillis = require('getTimestampMillis');

const setInWindow = require('setInWindow');
const localStorage = require('localStorage');

const LocalStorageConsentStates_Base64 = localStorage.getItem(
    'ML_CookieMan_ConsentState'
);

const CookieManToken = data.CookieManToken;
const CookieManLanguage = data.CookieManLanguage;
const CookieManHost = data.CookieManHost;
let url = 'https://' + CookieManHost + '.cookieman.it/cookiemanmanager_II.js';

const waitForUpdate = data.waitForUpdate;
const urlPassthrough = data.urlPassthrough;
const adsDataRedaction = data.adsDataRedaction;

//Debug mode
const log = data.debug ? logToConsole : () => {};

function CookieMan_SetDefaultConsent() {
    let defaultConsent = {
        ad_storage: 'denied',
        ad_user_data: 'denied',
        ad_personalization: 'denied',
        analytics_storage: 'denied',
        functionality_storage: 'denied',
        personalization_storage: 'denied',
        security_storage: 'denied',
    };

    log('CookieMan - Set CookieManDefault Consent', defaultConsent);

    consentDefaultStates.push(defaultConsent);
    setDefaultConsentState(defaultConsent);
}

function CookieMan_UpdateConsent(opts) {
    let consentStates = {
        ad_storage: opts.ad_storage ? opts.ad_storage : 'denied',
        ad_user_data: opts.ad_user_data ? opts.ad_user_data : 'denied',
        ad_personalization: opts.ad_personalization
            ? opts.ad_personalization
            : 'denied',
        analytics_storage: opts.analytics_storage
            ? opts.analytics_storage
            : 'denied',
        functionality_storage: opts.functionality_storage
            ? opts.functionality_storage
            : 'denied',
        personalization_storage: opts.personalization_storage
            ? opts.personalization_storage
            : 'denied',
        security_storage: opts.security_storage
            ? opts.security_storage
            : 'denied',
    };

    log('CookieMan - Update Consent States', consentStates);
    
    updateConsentState(consentStates);
}

const CookieMan_GetRegionArray = (regionString) => {
    return regionString
        .split(',')
        .map((region) => region.trim())
        .filter((region) => region.length !== 0);
};

if (data.regionSettings && data.regionSettings.length > 0) {
    data.regionSettings.forEach((regionSetting) => {
        let consentStates = {
            ad_storage: regionSetting.defaultConsentMarketing,
            ad_user_data: regionSetting.defaultConsentMarketing,
            ad_personalization: regionSetting.defaultConsentMarketing,
            analytics_storage: regionSetting.defaultConsentMeasure,
            functionality_storage: regionSetting.defaultConsentFunctionality,
            personalization_storage: regionSetting.defaultConsentExperience,
            security_storage: regionSetting.defaultConsentFunctionality,
        };

        if (waitForUpdate > 0) {
            consentStates.wait_for_update = waitForUpdate;
        }

        const regionArray = CookieMan_GetRegionArray(regionSetting.region);

        if (regionArray.length > 0) {
            consentStates.region = regionArray;
        } else {
            log('CookieMan - Global consent found');
            hasDefaultConsentState = true;
        }

        setDefaultConsentState(consentStates);
        consentDefaultStates.push(consentStates);
    });
}

if (!hasDefaultConsentState) {
    CookieMan_SetDefaultConsent();
}

if (LocalStorageConsentStates_Base64) {
    const defaultConsent = JSON.parse(
        fromBase64(LocalStorageConsentStates_Base64)
    );

    log('CookieMan - Update consent states found in local storage');

    CookieMan_UpdateConsent(defaultConsent);
}

function GTM_ConsentModeUpdate(consentStates) {
    CookieMan_UpdateConsent(consentStates);
}

// If the script loaded successfully
const onSuccess = () => {
    log('CookieMan - CookieMan script loaded.');
    data.gtmOnSuccess();
};

// If the script fails to load
const onFailure = () => {
    log('CookieMan - CookieMan script loading error.');
    data.gtmOnFailure();
};

//Inject the CookieMan script
if (queryPermission('inject_script', url)) {
    let cacheBreakUrl = url + '?' + 'cb=' + getTimestampMillis();

    log('CookieMan - Script to inject ', cacheBreakUrl);

    setInWindow('ML_CookieMan_DataLayerGTM', {
        token: CookieManToken,
        lang: CookieManLanguage,
        consentModeDefault: consentDefaultStates,
    });

    setInWindow(
        'ML_CookieMan_DataLayerGTM.consentModeUpdate',
        GTM_ConsentModeUpdate
    );

    gtagSet({
        url_passthrough: urlPassthrough === true,
        ads_data_redaction: adsDataRedaction === true,
        'developer_id.dODdmYz': true,
    });

    injectScript(cacheBreakUrl, onSuccess, onFailure);
} else {
    log('CookieMan - Permission denied');
    data.gtmOnFailure();
}


